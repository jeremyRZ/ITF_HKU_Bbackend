<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.itf.backend.mapper.ReportMapper">
    <insert id="add" useGeneratedKeys="true" keyProperty="id">
        insert into user(name, password,post,emailAddress,cellNumber) values (#{name},#{password},#{post},#{emailAddress},#{cellNumber})
    </insert>


    <select id="listReportByTaskID" resultType="com.itf.backend.model.Report">
        select * from inspection
        <where>
            <!-- 重复说一下，id 不能是 int，int 默认是 0，不可能是 null, 而 Integer 可以是 null-->
            <if test="taskID!=null">
                taskID=#{taskID}
            </if>
            <!--            <if test="name!=null">-->
            <!--                and name=#{name}-->
            <!--            </if>-->
        </where>
    </select>

</mapper>